{{#if error}}
<div class="alert alert-danger">
  {{error}}
  <a class="close" data-dismiss="alert" aria-label="close">&times;</a>
</div>
{{/if}}
{{#each errors}}
<div class="alert alert-danger">
  {{text}}
  <a class="close" data-dismiss="alert" aria-label="close">&times;</a>
</div>
{{else}}

{{/each}}